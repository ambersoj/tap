{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Kernel Debugging",
            "type": "cppdbg",
            "request": "attach",
            "program": "/usr/local/tap/tun.ko",
            "processId": "pick",
            "MIMode": "gdb",
            "setupCommands": [
                { "text": "-enable-pretty-printing" }
            ],
            "sourceFileMap": {
                "/usr/src/linux-source-6.8.0/linux-source-6.8.0": "/usr/local/tap/"
            }
        }
    ]
}
